  "version": "0.1.1",
  "main": "build/cmjs/index.js",
  "module": "build/es6/index.js",
  "dependencies": {
    "browserify": "^16.2.3",
    "typescript": "^3.3.3"
  },
  "scripts": {
    "build:cmjs": "tsc -P tsconfig.json",
    "build:es6": "tsc -P tsconfig.json --module ES6 --outDir build/es6",
    "build:web": "browserify build/cmjs/index.js --standalone enhanced-fetch -o build/web/bundle.js",
    "build": "npm run build:cmjs; npm run build:es6",
    "pactch": "npm version patch -m 'update version %s'"
  }
}
