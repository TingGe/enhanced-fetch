  "version": "0.0.1",
  "main": "lib/cmjs/index.js",
  "module": "lib/es6/index.js",
  "scripts": {
    "build:cmjs": "tsc -P tsconfig.json",
    "build:es6": "tsc -P tsconfig.json --module ES6 --outDir lib/es6",
    "build:web": "browserify lib/cmjs/index.js --standalone enhanced-fetch -o lib/web/bundle.js",
    "build": "npm run build:cmjs; npm run build:es6; npm run build:web",
    "pactch": "npm version patch -m 'update version %s'",
    "test": "ts-node src/*.spec.ts"
  },
  "devDependencies": {
    "ts-node": "^8.0.2",
    "browserify": "^16.2.3",
    "typescript": "^3.3.3"
  }
}
